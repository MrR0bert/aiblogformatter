{
  "name": "aiblogformatter",
  "displayName": "AIBLogFormatter",
  "description": "Remove clutter from the Customization.log generated by Azure Image Builder",
  "version": "1.0.0",
  "publisher": "MrR0b3rt",
  "author": {
    "name": "MrR0b3rt"
  },
  "engines": {
    "vscode": "^1.83.0"
  },
  "categories": [
    "Programming Languages",
    "Formatters"
  ],
  "repository": {
    "type": "Git",
    "url": "https://github.com/MrR0bert/vscode-aiblogformatter.git"
  },
  "activationEvents": [],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "aiblogformatter.format",
        "title": "Format Azure Image Builder log"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.83.0",
    "@types/mocha": "^10.0.2",
    "@types/node": "18.x",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "eslint": "^8.50.0",
    "glob": "^10.3.3",
    "mocha": "^10.2.0",
    "typescript": "^5.2.2",
    "@vscode/test-electron": "^2.3.4"
  }
}
